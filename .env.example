# SwarAI Environment Variables

# PostgreSQL Database Configuration
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/swarai

# Ollama Configuration
OLLAMA_BASE_URL=http://localhost:11434
OLLAMA_MODEL=llama3.2

# Gemini API Configuration (comma-separated for multiple keys)
GEMINI_API_KEYS=your-gemini-api-key-1,your-gemini-api-key-2
GEMINI_API_ENDPOINT=https://generativelanguage.googleapis.com/v1beta/models/gemini-pro:generateContent

# OpenAI API Configuration for Whisper
WHISPER_API_KEY=your-openai-api-key

# Server Configuration
PORT=8000
HOST=0.0.0.0
CORS_ORIGINS=http://localhost:3000,https://yourdomain.com

# Security Settings
JWT_SECRET=change-this-to-a-random-secure-secret
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=60

# Logging and Monitoring
LOG_LEVEL=INFO
ENABLE_TELEMETRY=false