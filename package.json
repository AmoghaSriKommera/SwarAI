{
  "name": "swarai",
  "version": "1.0.0",
  "description": "Voice-enabled AI assistant with hybrid LLM support",
  "main": "main.py",
  "scripts": {
    "start": "python main.py",
    "dev": "python main.py",
    "backend": "cd backend && python -m uvicorn main:app --reload --host 0.0.0.0 --port 8000",
    "frontend": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "install-deps": "pip install -r requirements.txt && cd frontend && npm install",
    "desktop-dev": "cd frontend && npm run electron-dev",
    "build-desktop": "cd frontend && npm run dist",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "ai",
    "voice",
    "assistant",
    "llm",
    "ollama",
    "gemini",
    "fastapi",
    "react",
    "typescript"
  ],
  "author": "SwarAI Team",
  "license": "MIT",
  "engines": {
    "node": ">=16.0.0",
    "python": ">=3.8.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/your-username/swarai.git"
  }
}
